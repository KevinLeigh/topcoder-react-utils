{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {}
    },
    "jsx-a11y/anchor-is-valid": ["error", {
      "components": [],
      "specialLink": ["hrefLeft", "hrefRight"],
      "aspects": ["invalidHref", "noHref", "preferButton"]
    }],
    "react/jsx-one-expression-per-line": false
  }
}